{
  "name": "AnkaPulse-backend",
  "version": "1.0.0",
  "description": "AnkaPulse Backend API",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "build:prod": "npm run db:generate && npm run build",
    "start": "npm run db:push && node dist/app.js",
    "start:prod": "prisma db push --accept-data-loss && NODE_ENV=production node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lemonsqueezy/lemonsqueezy.js": "^4.0.0",
    "@prisma/client": "^5.3.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.14.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.3.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.9.0",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "resend": "^6.1.2",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.12",
    "prisma": "^5.3.1",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}